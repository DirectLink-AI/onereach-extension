{
  "name": "OneReach.ai Keyboard Shortcuts",
  "version": "1.0",
  "description": "Add navigation to OneReach.ai",
  "manifest_version": 2,
  "icons": {
    "16": "images/favicon-2.png",
    "48": "images/favicon-1.png",
    "128": "images/favicon-1.png"
  },
  "permissions": ["*://*.onereach.ai/*"],
  "web_accessible_resources": [
    "js/*",
    "css/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.onereach.ai/*"],
      // "css": ["css/dark.css"],
      "js": ["js/arrive.js", "js/main.js"]
    }
  ]
}
